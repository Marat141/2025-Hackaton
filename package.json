		"@tailwindcss/vite": "^4.1.17",
		"@types/node": "^22",
		"autoprefixer": "^10.4.22",
		"dotenv-cli": "^11.0.0",
		"drizzle-kit": "^0.31.7",
		"drizzle-orm": "^0.44.7",
		"postcss": "^8.5.6",
		"svelte": "^5.43.8",
		"tailwindcss": "^4.1.17",
		"typescript": "^5.9.3",
		"vite": "^7.2.2"
	},
	"dependencies": {
		"dotenv": "^17.2.3",
		"groq-sdk": "^0.37.0",
